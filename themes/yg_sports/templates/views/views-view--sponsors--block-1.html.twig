<div id="sponsors">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-md-12">
        <div class="upcoming-text text-center">
          <div class="section-title">
              <h2>{{ 'SPONSORS'|t }}</h2>
          </div>
          {% for result in view.result %}
            <div class="col-md-2 col-sm-4 col-xs-6 matches">
               <div class="matche-image">
				         <img src="{{ file_url(view.field.uri.value(view.result[loop.index-1])) }}" class="img-responsive" alt="{{ view.field.title.value(view.result[loop.index-1]) }}"/>
               </div>
			      </div>
          {% endfor %}
        </div>
      </div>
    </div>
  </div>
</div>
